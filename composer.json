{
    "name": "sammyjo20/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/Sammyjo20/zipper",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Developer"
        },
        {
            "name": "Sam CarrÃ©",
            "email": "sam@codepotato.co.uk",
            "role": "Developer",
            "homepage": "https://codepotato.co.uk"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-zip": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sammyjo20\\Zipper\\": "src/Sammyjo20/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sammyjo20\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sammyjo20\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Sammyjo20\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
